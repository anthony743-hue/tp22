select employees.first_name, employees.last_name, departments.dept_name 
FROM departments JOIN
dept_emp ON departments.dept_no = dept_emp.dept_no
JOIN employees ON dept_emp.emp_no = employees.emp_no WHERE departments.dept_name LIKE ;

SELECT * FROM employees WHERE first_name LIKE 'Aamer' 
    AND (2025 - YEAR(birth_date)) >= 61 AND (2025 - YEAR(birth_date)) <= 67